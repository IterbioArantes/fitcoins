{
  "expo": {
    "name": "fitcoins",
    "slug": "fitcoins",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/Fit.png",
    "splash": {
      "image": "./assets/logos/splash_FIT.png",
      "resizeMode": "contain",
      "backgroundColor": "#C5A2CB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/Fit.png",
        "backgroundColor": "#C5A2CB"
      }
    },
    "web": {
      "favicon": "./assets/logos/Fit.png"
    }
  }
}
